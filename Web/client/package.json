{
  "name": "drawmeapicture",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "ng serve --open",
    "format": "prettier --config ../.prettierrc.yaml --write \"src/**/*\"",
    "build": "ng build",
    "test": "ng test",
    "coverage": "ng test --code-coverage --sourceMap=true",
    "lint": "ng lint --fix --type-check --format stylish",
    "start:electron": "ng build --base-href ./ && electron .",
    "build:electron": "ng build --prod && electron-packager .  --asar --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite --out=./build --app-version=1.0 --icon=./src/favicon.ico --appname=\"Draw Me a Picture!\" --win32metadata.CompanyName=\"Polytechnique Montreal\" --win32metadata.FileDescription=\"Draw Me a Picture!\" --win32metadata.ProductName=\"Draw Me a Picture!\""
  },
  "private": true,
  "dependencies": {
    "@angular/animation": "^4.0.0-beta.8",
    "@angular/animations": "~9.1.13",
    "@angular/cdk": "~9.2.4",
    "@angular/common": "~9.1.13",
    "@angular/compiler": "~9.1.13",
    "@angular/core": "~9.1.13",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~9.1.13",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.13",
    "@angular/platform-browser-dynamic": "~9.1.13",
    "@angular/router": "~9.1.13",
    "canvas-confetti": "^1.4.0",
    "coverage": "^0.4.1",
    "firebase": "^8.2.7",
    "igniteui-angular": "^11.1.7",
    "ng-image-slider": "^2.6.4",
    "ngx-electron": "^2.2.0",
    "prettier": "^1.19.1",
    "rxjs": "~6.6.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.13",
    "@angular/cli": "~9.1.13",
    "@angular/compiler-cli": "~9.1.13",
    "@angular/language-service": "~9.1.13",
    "@types/canvas-confetti": "^1.4.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.7",
    "codelyzer": "^5.1.2",
    "electron": "^11.2.1",
    "electron-packager": "^15.2.0",
    "jasmine": "^3.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "~3.8.3"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "directories": {
    "buildResources": "build",
    "app": "build"
  }
}
